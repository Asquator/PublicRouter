                     <font>
                        <Font size="39.0" />
                     </font>
                  </Label>
                  <Spinner fx:id="minutesSpinner" editable="true" prefHeight="48.0" prefWidth="98.0" />
               </children>
            </HBox>
            <Separator prefHeight="30.0" prefWidth="224.0" visible="false">
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
               <effect>
                  <Blend />
               </effect>
            </Separator>
            <DatePicker fx:id="datePicker" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="63.0" prefWidth="224.0" VBox.vgrow="SOMETIMES" />
