      </TextField>
      <TextField fx:id="destinationField" prefHeight="32.0" prefWidth="224.0" promptText="Destination">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Separator prefHeight="16.0" prefWidth="224.0" visible="false">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <effect>
            <Blend />
         </effect>
      </Separator>
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#searchClicked" prefHeight="24.0" prefWidth="91.0" text="ROUTE">
         <font>
            <Font name="Cantarell Regular" size="17.0" />
         </font>
      </Button>
      <ListView fx:id="solutionListView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="399.0" prefWidth="224.0" VBox.vgrow="ALWAYS" />
   </children>
